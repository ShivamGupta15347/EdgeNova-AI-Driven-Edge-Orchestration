# Executor Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit your .env file to version control!

# MQTT Configuration
MQTT_BROKER_URL=mqtts://your-mqtt-broker.example.com:443
MQTT_USERNAME=your_mqtt_username
MQTT_PASSWORD=your_mqtt_password
MQTT_TOPIC=commands
MQTT_CLIENT_ID=go-mqtt-ai-executor

# AI Model Configuration
AI_MODEL_URL=http://localhost:8080/generate-shell

# Example:
# MQTT_BROKER_URL=mqtts://broker.hivemq.com:8883
# MQTT_USERNAME=user123
# MQTT_PASSWORD=securePassword123
# MQTT_TOPIC=device/commands
# MQTT_CLIENT_ID=executor-001
